{
  "name": "@corbat-tech/coco",
  "version": "1.5.0",
  "description": "Autonomous Coding Agent with Self-Review, Quality Convergence, and Production-Ready Output",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "coco": "./dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src test",
    "lint:fix": "oxlint src test --fix",
    "format": "oxfmt --check src test",
    "format:fix": "oxfmt --write src test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "benchmark": "tsx test/benchmarks/quality-benchmark.ts",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "prepublishOnly": "pnpm build",
    "check": "pnpm typecheck && pnpm lint && pnpm test"
  },
  "keywords": [
    "ai",
    "agent",
    "coding",
    "autonomous",
    "development",
    "code-generation",
    "quality",
    "testing",
    "cli",
    "llm",
    "code-review",
    "code-quality",
    "self-review",
    "multi-agent",
    "convergence",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/corbat/corbat-coco.git"
  },
  "homepage": "https://github.com/corbat/corbat-coco#readme",
  "bugs": {
    "url": "https://github.com/corbat/corbat-coco/issues"
  },
  "author": "Corbat",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@clack/prompts": "^1.0.0",
    "@google/generative-ai": "^0.24.1",
    "@types/diff": "^8.0.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript-eslint/typescript-estree": "^8.54.0",
    "ansi-escapes": "^7.3.0",
    "chalk": "^5.4.0",
    "commander": "^14.0.3",
    "diff": "^8.0.3",
    "execa": "^9.5.0",
    "glob": "^13.0.1",
    "highlight.js": "^11.11.1",
    "json5": "^2.2.3",
    "marked": "^15.0.0",
    "marked-terminal": "^7.0.0",
    "minimatch": "^10.1.2",
    "openai": "^6.17.0",
    "ora": "^9.2.0",
    "simple-git": "^3.27.0",
    "string-width": "^8.1.1",
    "tslog": "^4.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^3.0.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
